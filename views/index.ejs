<!DOCTYPE html>
<html lang="bg">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="/styles/footer.css" />
    <link rel="stylesheet" href="/styles/global.css" />
    <link rel="stylesheet" href="/styles/responsive/nav.css" />
    <link rel="stylesheet" href="/styles/nav.css" />
    <link rel="stylesheet" href="/styles/index.css" />
    <link rel="stylesheet" href="/styles/responsive/index.css" />
    <link rel="stylesheet" href="/styles/responsive/footer.css" />
    <link rel="stylesheet" href="/styles/loadingScreen.css" />
    <link rel="shortcut icon" href="/img/logo.png" />
    <title>Welldoers</title>
  </head>
  <body>
    <%- include('partials/loading-screen') %>

    <div class="cookies-info-container"></div>

    <%- include('partials/header') %>

    <section class="slideshow">
      <video playsinline autoplay muted loop src="<%= videoUrl %>"></video>
      <span id="typed"></span>
      <div class="arrow-down"><p>&darr;</p></div>
    </section>

    <section class="welcomeSection">
      <img src="/img/wakeboarder1.png" alt="" />
      <div class="welcome-content">
        <h1><%= data.index.header1 %></h1>
        <p><%= data.index.paragraph1 %></p>
      </div>
    </section>

    <section class="lines">
      <div class="line-1">
        <div class="line-1-left">
          <h1><%= data.index.line1Heading %></h1>
          <p><%= data.index.line1Paragraph %></p>
        </div>
        <div class="line-1-right">
          <img src="/img/noimg.svg" alt="img" />
        </div>
      </div>
      <div class="line-2">
        <div class="line-2-right">
          <h1><%= data.index.line2Heading %></h1>
          <p><%= data.index.line2Paragraph %></p>
        </div>
        <div class="line-2-left">
          <img src="/img/noimg.svg" alt="img" />
        </div>
      </div>
    </section>

    <section class="galery"></section>

    <%- include('partials/footer') %>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.11.5/gsap.min.js"></script>
    <!-- Include the latest version of GSAP -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.9.1/ScrollTrigger.min.js"></script>
    <!-- Include ScrollTrigger -->
    <script src="https://unpkg.com/typed.js@2.0.16/dist/typed.umd.js"></script>
    <!-- Include Typed.js -->
    <script src="/JS/mobileMenu.js"></script>
    <script src="/JS/loadingAnimation.js"></script>
    <script src="/JS/indexArrowAnimation.js"></script>
    <script src="/JS/indexTypingAnimation.js"></script>
    <script src="/JS/linksTransfer.js"></script>
    <script src="/JS/GSAP/index.js"></script>
  </body>
</html>
